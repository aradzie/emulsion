{
  "private": true,
  "name": "emulsion",
  "version": "1.0.0",
  "description": "Photo emulsion.",
  "author": "Aliaksandr Radzivanovich",
  "license": "GPL-3",
  "scripts": {
    "precompile": "node generate.js",
    "compile": "tsc",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint-fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "stylelint": "stylelint src/**/*.css",
    "stylelint-fix": "stylelint src/**/*.css --fix",
    "format": "prettier --write .",
    "build-prod": "env NODE_ENV=production webpack",
    "build-dev": "env NODE_ENV=development webpack",
    "build-watch": "env NODE_ENV=development webpack --watch",
    "publish": "gh-pages -d build"
  },
  "dependencies": {
    "clsx": "^1.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tslib": "^2.4.1"
  },
  "devDependencies": {
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "compression-webpack-plugin": "^10.0.0",
    "css-loader": "^6.7.3",
    "eslint": "^8.31.0",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-restricted-globals": "^0.2.0",
    "fast-glob": "^3.2.12",
    "gh-pages": "^4.0.0",
    "html-webpack-plugin": "^5.5.0",
    "prettier": "^2.8.2",
    "source-map-loader": "^4.0.1",
    "style-loader": "^3.3.1",
    "stylelint": "^14.16.1",
    "stylelint-config-recommended": "^9.0.0",
    "stylelint-order": "^6.0.1",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  }
}
